<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ImagesFragment">

    <ScrollView
        android:layout_width="411dp"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="655dp"
                android:layout_weight="1"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="match_parent"
                    android:layout_height="135dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg3"
                    app:srcCompat="@drawable/cimg3" />

                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="match_parent"
                    android:layout_height="139dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg1"
                    app:srcCompat="@drawable/cimg1" />

                <ImageView
                    android:id="@+id/imageView21"
                    android:layout_width="match_parent"
                    android:layout_height="135dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg9"
                    app:srcCompat="@drawable/cimg9" />

                <ImageView
                    android:id="@+id/imageView23"
                    android:layout_width="234dp"
                    android:layout_height="135dp"
                    android:cropToPadding="false"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg10"
                    app:srcCompat="@drawable/cimg10" />

                <ImageView
                    android:id="@+id/imageView22"
                    android:layout_width="wrap_content"
                    android:layout_height="135dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg2"
                    app:srcCompat="@drawable/cimg2"
                    tools:srcCompat="@drawable/cimg2" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <ImageView
                    android:id="@+id/imageView15"
                    android:layout_width="match_parent"
                    android:layout_height="135dp"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg6"
                    app:srcCompat="@drawable/cimg6" />

                <ImageView
                    android:id="@+id/imageView12"
                    android:layout_width="match_parent"
                    android:layout_height="139dp"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg5"
                    app:srcCompat="@drawable/cimg5" />

                <ImageView
                    android:id="@+id/imageView17"
                    android:layout_width="match_parent"
                    android:layout_height="135dp"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg4"
                    app:srcCompat="@drawable/cimg4" />

                <ImageView
                    android:id="@+id/imageView20"
                    android:layout_width="match_parent"
                    android:layout_height="134dp"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg8"
                    app:srcCompat="@drawable/cimg8" />

                <ImageView
                    android:id="@+id/imageView24"
                    android:layout_width="match_parent"
                    android:layout_height="112dp"
                    android:layout_weight="1"
                    android:scaleType="fitXY"
                    android:src="@drawable/cimg7" />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</FrameLayout>